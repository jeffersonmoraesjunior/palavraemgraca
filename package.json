{
  "name": "promise-box",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "compile": "tsc",
    "prebuild": "npm run compile",
    "image-optimize": "node scripts/optimizeImages.js",
    "build": "npm run generate:rss && npm run image-optimize && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:wp": "vite build --outDir wordpress-build --config vite.config.wp.ts",
    "generate-sitemap": "node scripts/generateSitemap.js",
    "build:with-sitemap": "npm run generate-sitemap && npm run build",
    "generate:rss": "node scripts/generateRssFeed.js",
    "generate:favicon": "node scripts/generateFavicon.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@types/react-helmet": "^6.1.11",
    "feed": "^4.2.2",
    "lucide-react": "^0.344.0",
    "openai": "^4.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.2.0",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/node": "^20.17.30",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "caniuse-lite": "^1.0.30001705",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "fast-glob": "^3.3.3",
    "globals": "^15.9.0",
    "postcss": "^8.5.3",
    "sharp": "^0.33.5",
    "tailwindcss": "^4.1.3",
    "terser": "^5.39.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
