{
  "name": "promise-box",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "compile": "tsc",
    "prebuild": "npm run compile",
    "image-optimize": "node scripts/optimizeImages.js",
    "copy-content": "node scripts/copyContent.js",
    "build": "npm run generate:rss && npm run image-optimize && npm run copy-content && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:wp": "vite build --outDir wordpress-build --config vite.config.wp.ts",
    "generate-sitemap": "node scripts/generateSitemap.js",
    "build:with-sitemap": "npm run generate-sitemap && npm run build",
    "generate:rss": "node scripts/generateRssFeed.js",
    "generate:favicon": "node scripts/generateFavicon.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@types/react-helmet": "^6.1.11",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.344.0",
    "openai": "^4.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.22.3",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vercel/node": "^5.1.14",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.1",
    "terser": "^5.39.0",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  }
}
